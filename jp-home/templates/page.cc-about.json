        },
        "text_hNBU4t": {
          "type": "text",
          "settings": {
            "title": "How long does it take to prepare my order?",
            "subtitle": "",
            "icon": "no-icon",
            "button_label": "",
            "button_link": "",
            "body": "<p>To ensure your meals are freshly prepared, please allow for 30 to 45 minutes preparation time after an order is placed.</p>",
